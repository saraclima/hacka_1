<h2>Map</h2>

    <div id="map" style="margin-left:15px; width: 90%; height: 400px"></div>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDsBL7oBswxYKSnmKGuCWGmv5oiNSCumCk"></script>

    <script type="text/javascript">
        var myLatlng = new google.maps.LatLng(48.852937,2.364178);

        var myOptions = {
            zoom: 14,
            center: myLatlng,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            styles: []
        };

        var map = new google.maps.Map(document.getElementById('map'), myOptions);
        var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title: "You are here!"
        });
    </script>




 



